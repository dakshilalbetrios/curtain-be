{
  "name": "clinovia-be",
  "version": "1.0.0",
  "description": "Clinovia Backend REST APIs ",
  "main": "app.js",
  "scripts": {
    "db:setup": "node scripts/setup-database.js",
    "db:init": "node src/database/core/run.migration.js --init",
    "db:migrate": "node src/database/core/run.migration.js",
    "db:rollback": "node src/database/core/run.migration.js --rollback",
    "db:rollback:all": "node src/database/core/run.migration.js --rollback all",
    "db:rollback:today": "node src/database/core/run.migration.js --rollback today",
    "db:destroy": "node src/database/core/run.migration.js --destroy",
    "db:make": "knex migrate:make -x js --knexfile src/database/core/knex/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node src/app.js",
    "start:dev": "nodemon src/app.js",
    "pretty": "prettier --write \"./**/*.{js,jsx,json}\"",
    "table-module-make": "node scripts/generate-module.js",
    "table-module-delete": "node scripts/delete-module.js"
  },
  "author": "albetrios",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.844.0",
    "@aws-sdk/s3-request-presigner": "^3.844.0",
    "@sendgrid/mail": "8.1.5",
    "axios": "1.9.0",
    "bcryptjs": "3.0.2",
    "body-parser": "2.2.0",
    "bull": "4.16.5",
    "compression": "1.8.0",
    "cookie-parser": "^1.4.7",
    "cors": "2.8.5",
    "dotenv": "16.5.0",
    "express": "5.1.0",
    "express-mung": "0.5.1",
    "express-rate-limit": "7.5.0",
    "helmet": "8.1.0",
    "joi": "17.13.3",
    "jsonwebtoken": "9.0.2",
    "knex": "3.1.0",
    "knex-on-duplicate-update": "2.3.3",
    "lodash": "4.17.21",
    "moment-timezone": "0.5.48",
    "mysql2": "3.14.1",
    "node-cron": "3.0.3",
    "pg": "8.15.6",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "sql-formatter": "15.6.1",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "eslint": "9.25.1",
    "eslint-config-prettier": "10.1.2",
    "eslint-plugin-prettier": "5.2.6",
    "nodemon": "3.1.10",
    "prettier": "3.5.3"
  }
}
